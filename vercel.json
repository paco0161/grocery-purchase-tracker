{
    "version": 0,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "api/flask_app/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/flask_app/index.py"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ]
}
  